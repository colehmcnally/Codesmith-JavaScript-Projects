function goodKeys(object, callback) {
  // create an array to store our results
  const results = [];
  // for loop to iterate through the object
  for (let key in object) {
    // if the returned value from the callback is true (the value in the object's key:value pair), push the current key from the object to results array
    if (callback(object[key]) === true) {
      results.push(key);
    }
  }
// return results
  return results;
}

// Uncomment these to check your work!
const sunny = { mac: 'priest', dennis: 'calculator', charlie: 'birdlaw', dee: 'bird', frank: 'warthog' };
function startsWithBird(str) { return str.slice(0, 4).toLowerCase() === 'bird'; };
console.log(goodKeys(sunny, startsWithBird)); // should log: ['charlie', 'dee']